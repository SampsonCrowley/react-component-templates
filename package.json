{
  "name": "react-component-templates",
  "version": "4.0.4",
  "description": "Default React Components to use across apps",
  "main": "index.js",
  "scripts": {
    "start": "yarn styles && node scripts/start.js",
    "build": "yarn styles && node scripts/build.js && cp package.json build/package.json",
    "demo": "yarn styles && node scripts/demo.js",
    "release": "yarn run build && npm publish build",
    "styles": "node scripts/create-styles run",
    "styles:watch": "shopt -s globstar && stylus -I src -u nib -w ./src/**/*.styl",
    "styles:watch:scripted": "node scripts/create-styles watch",
    "test": "yarn styles && node scripts/test.js --env=jsdom",
    "login": "npm adduser"
  },
  "files": [
    "*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SampsonCrowley/react-component-templates.git"
  },
  "keywords": [
    "react",
    "component",
    "library",
    "templates"
  ],
  "author": "Sampson Crowley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SampsonCrowley/react-component-templates/issues"
  },
  "homepage": "https://github.com/SampsonCrowley/react-component-templates#readme",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "autoprefixer-stylus": "^0.14.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "brotli": "^1.3.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "focus-within": "^2.0.0",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "load-awesome-react-components": "^1.0.9",
    "load-awesome-relative": "^0.0.3",
    "moment": "^2.22.2",
    "nib": "^1.1.2",
    "node-glob": "^1.2.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "^15.6.1",
    "raf": "3.4.0",
    "react": "^16.4.0",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-select-fast-filter-options": "^0.2.3",
    "react-test-renderer": "^16.4.2",
    "react-virtualized-select": "^3.1.3",
    "style-loader": "0.19.0",
    "stylus": "^0.54.5",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "peerDependencies": {
    "focus-within": "^2.0.0",
    "load-awesome-react-components": "^1.0.9",
    "load-awesome-relative": "^0.0.3",
    "moment": "^2.22.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.3.1",
    "react-select-fast-filter-options": "^0.2.3",
    "react-virtualized-select": "^3.1.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/src/lib/polyfills/index.js",
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
