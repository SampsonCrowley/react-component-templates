{
  "name": "react-component-templates",
  "version": "7.0.12",
  "description": "Default React Components to use across apps",
  "main": "build/index.js",
  "scripts": {
    "start": "yarn styles && node scripts/start.js",
    "build": "yarn styles && node scripts/build.js && cp package.json build/package.json",
    "demo": "yarn styles && node scripts/demo.js",
    "release": "yarn run build && npm publish build",
    "styles": "node scripts/create-styles run",
    "styles:watch": "shopt -s globstar && stylus -I src -u nib -w ./src/**/*.styl",
    "styles:watch:scripted": "node scripts/create-styles watch",
    "test": "yarn styles && node scripts/test.js --env=jsdom --no-cache",
    "login": "npm adduser"
  },
  "files": [
    "*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SampsonCrowley/react-component-templates.git"
  },
  "keywords": [
    "react",
    "component",
    "library",
    "templates"
  ],
  "author": "Sampson Crowley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SampsonCrowley/react-component-templates/issues"
  },
  "homepage": "https://github.com/SampsonCrowley/react-component-templates#readme",
